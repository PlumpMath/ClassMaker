<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://org/classupplier/0.7.5"
            class="org.classupplier.ClasSupplierPackage"
            genModel="model/ClasSupplier.genmodel"/>
   </extension>
   <extension
         id="domainNature"
         name="Domain Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.classupplier.impl.DomainProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         id="resourceBuilder"
         name="Model Resource Builder"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.classupplier.builders.ResourceBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="generatorBuilder"
         name="Code Generation Builder"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.classupplier.builders.GeneratorBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="exportBuilder"
         name="Plug-in Export Builder"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.classupplier.builders.ExportingBuilder">
         </run></builder>
   </extension>
   <extension
         id="loadBuilder"
         name="Bundle Load Builder"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
               class="org.classupplier.builders.LoadBuilder">
         </run>
      </builder>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.classupplier.ClasSupplier"
            class="org.classupplier.query.ClasSupplierAdapterFactory">
         <adapter
               type="org.eclipse.emf.query.conditions.eobjects.IEObjectSource">
         </adapter>
      </factory>
   </extension>

</plugin>
